{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "developmentClient": false,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "credentialsSource": "remote",
      "env": {
        "EXPO_USE_LOCAL_CLI": "1",
        "EXPO_NO_GIT_STATUS": "1",
        "EXPO_BARE": "1"
      },
      "cache": {
        "disabled": true
      },
      "environment": {
        "node": "20"
      }
    },
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "credentialsSource": "remote",
      "env": {
        "EXPO_USE_LOCAL_CLI": "1",
        "EXPO_NO_GIT_STATUS": "1"
      },
      "environment": {
        "node": "20"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
